
@using MovieProject.Models
@model IEnumerable<Category>

<div class="list-group">
    <a href="/" class="list-group-item list-group-item-action">
        All Categories
    </a>
 @foreach (var item in Model)
{
    <a asp-controller="Home" asp-action="Index" asp-route-id="@item.CategoryId" class="list-group-item list-group-item-action">@item.CategoryName</a>
}
</div>

